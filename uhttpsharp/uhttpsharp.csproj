	<PackageId>uhttpsharp</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="6.0.0.0" />
   </ItemGroup>

</Project>
